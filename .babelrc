{
  "compact": true,
  "minified": true,
  "sourceMap": true,
  "comments": false,
  "presets": [
    ["@babel/preset-env", { "useBuiltIns": "entry", "corejs": "core-js@3" }],
    "@babel/preset-flow"
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ],
    "@babel/plugin-syntax-flow",
    "@babel/plugin-proposal-class-properties"
  ]
}
